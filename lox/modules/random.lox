import math;

// Returns an integer between min and max (inclusive)
func integer(min, max) {
    return min + math.floor(rand() * (max - min + 1));
}

func float(min, max) {
    return min + rand() * (max - min);
}   

func choice(lst) {
    if (len(lst)== 0) {
        return nil;
    }
    var index = integer(0, len(lst) - 1);
    return lst[index];
}