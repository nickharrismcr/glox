for (var i = 0; i < 40; i=i+1) { 
    var line="";
    for (var j = 0; j < 80; j=j+1) { 
        var x0 = 4.0*(i-20)/40-1.0;
        var y0 = 4.0*(j-40)/80 ;
        var x=0;
        var y=0;
        var iteration = 0;
        var max_iteration = 1000;
        var xtemp;
        while ((x*x + y*y <= 4 ) and (iteration < max_iteration)) { 
            var xtemp = x*x - y*y + x0;
            y = 2.0*x*y+y0;
            x = xtemp;
            iteration = iteration + 1;
        }
        if (iteration == max_iteration) {
            line=line+" ";
        } else {
            line=line+"*";
        }
    }
    print line;
 }